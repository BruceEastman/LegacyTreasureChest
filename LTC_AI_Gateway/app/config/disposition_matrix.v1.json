      "id": "furniture_bulky_med_balanced",
      "priority": 100,
      "when": {
        "categories": ["Furniture"],
        "valueBand": ["MED", "HIGH", "*"],
        "bulky": true,
        "goals": ["balanced", "min_effort"],
        "chosenPaths": ["B", "C"]
      },
      "partnerTypes": [
        {
          "type": "consignment",
          "queries": [
            { "q": "{city} {region} furniture consignment", "weight": 1.0 },
            { "q": "{city} {region} home consignment store", "weight": 0.7 },
            { "q": "{city} {region} vintage consignment furniture", "weight": 0.5 }
          ],
          "trustGates": [
            "required:consignment_language",
            "established_language",
            "quality_curation_language"
          ],
          "rankWeights": {
            "trustScore": 0.35,
            "relevanceScore": 0.30,
            "distanceScore": 0.25,
            "reviewScore": 0.10
          }
        },
